reccion: {{ estudiante.direccion }}</h5>\r\n                            <br>\r\n                            <router-link :to=\"{name: 'edit', params: { id: estudiante.url }}\" class=\"btn btn-sm btn-primary\">Editar</router-link>\r\n                            <button class=\"btn btn-danger btn-sm ml-1\" v-on:click=\"deleteEstudiante(estudiante)\">Eliminar</button>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"col-md-8\">\r\n                        <div class=\"card-body\">\r\n                            <p class=\"card-text\">Ciudad: {{ estudiante.ciudad }}</p>\r\n                            <p class=\"card-text\">Tipo: {{ estudiante.tipo }}</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <p  v-if=\"estudiantes.length == 0\"> Sin Estudiantes</p>\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            estudiantes: []\r\n        }\r\n    },\r\n    created() {\r\n        this.all();\r\n    },\r\n    methods: {\r\n        deleteEstudiante: function(e) {\r\n            if (confirm('Eliminar ' + e.nombre)) {\r\n                axios.delete(e.url)\r\n                    .then( response => {\r\n                        this.all();\r\n                    });\r\n            }\r\n        },\r\n        all: function () {\r\n            axios.get('http://127.0.0.1:8000/edificios/')\r\n                .then( response => {\r\n                    this.estudiantes = response.data                    \r\n                });\r\n        }\r\n    },\r\n}\r\n</script>\r\n"]}]}