A;AACA,KAJA,CADA;AAMA,GAnBA;AAoBA,EAAA,OAAA,EAAA;AACA,IAAA,kBADA,gCACA;AAAA;;AACA,MAAA,KAAA,CACA,GADA,CACA,wCADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA;AASA,KAXA;AAYA,IAAA,MAAA,EAAA,gBAAA,CAAA,EAAA;AAAA;;AACA,WAAA,UAAA,CAAA,QAAA,GAAA,IAAA,CAAA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA;;AACA,YAAA,CAAA,MAAA,EAAA;AACA;AACA;;AACA,QAAA,KAAA,CAAA,GAAA,8CAAA,MAAA,CAAA,QAAA,CAAA,EAAA,QACA,MAAA,CAAA,QADA,EAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,YAAA;AACA,SALA;AAMA,OAXA;AAYA;AAzBA;AApBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"pt-5\">\r\n        <form @submit.prevent=\"create\" method=\"post\">\r\n            <div class=\"form-group\">\r\n                <label for=\"telefono\">Numero</label>\r\n                <input\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    id=\"telefono\"\r\n                    v-model=\"telefono.telefono\"\r\n                    v-validate=\"'required'\"\r\n                    name=\"telefono\"\r\n                    placeholder=\"Ingres telefono\"\r\n                    :class=\"{'is-invalid': errors.has('telefono.telefono') && submitted}\">\r\n                <div class=\"invalid-feedback\">\r\n                    Please provide a valid name.\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n                <label for=\"tipo\">Tipo</label>\r\n                <input\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    id=\"Tipo\"\r\n                    v-model=\"telefono.tipo\"\r\n                    v-validate=\"'required'\"\r\n                    name=\"tipo\"\r\n                    placeholder=\"Ingrese Tipo\"\r\n                    :class=\"{'is-invalid': errors.has('telefono.tipo') && submitted}\">\r\n                <div class=\"invalid-feedback\">\r\n                    Please provide a valid apellido.\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n                <label for=\"estudiante\">Estudiante</label>\r\n                <select v-model=\"telefono.estudiante\">\r\n                            <option v-for=\"e in estudiantesList\" :key=\"e.url\" :value=\"e.url\">{{ e.nombre }} {{ e.apellido }}</option>\r\n                        </select>\r\n            </div>\r\n\r\n            <button type=\"submit\" class=\"btn btn-primary\">Crear</button>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            telefono: {\r\n                telefono: '',\r\n                tipo: '',\r\n                estudiante: '',\r\n            },\r\n            estudiantesList: [],\r\n            submitted: false\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getEstudiantesList(),\r\n        axios.get('http://127.0.0.1:8000/api/numerost/' + this.$route.params.id + '/')\r\n            .then( response => {\r\n                console.log(response.data)\r\n                this.telefono = response.data\r\n        });\r\n    },\r\n    methods: {\r\n      getEstudiantesList() {\r\n            axios\r\n            .get('http://127.0.0.1:8000/api/estudiantes/')\r\n            .then(response => {\r\n                this.estudiantesList = response.data\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n            })\r\n\r\n        },\r\n        create: function (e) {\r\n            this.$validator.validate().then(result => {\r\n                this.submitted = true;\r\n                if (!result) {\r\n                    return;\r\n                }\r\n                axios.put(`http://127.0.0.1:8000/api/numerost/${this.telefono.id}/`,\r\n                        this.telefono\r\n                    )\r\n                    .then(response => {\r\n                        this.$router.push('/telefonos');\r\n                    })\r\n            });\r\n        }\r\n    },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components"}]}